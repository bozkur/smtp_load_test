        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="BeanShell Sampler" enabled="true">
          <stringProp name="BeanShellSampler.query">import org.example.*;

ServerInformation serverInformation = new ServerInformation(&quot;${host}&quot;, 25, false);
SmtpUser smtpUser = new SmtpUser(&quot;${fromUserName}&quot;, &quot;123456&quot;);
MessageSendOperation sendOperation = new MessageSendOperation(serverInformation, smtpUser);
sendOperation.createSession();
String fromAddress = String.format(&quot;%s@%s&quot;, new Object[] {&quot;${fromUserName}&quot;, &quot;${domain}&quot;});
String toAddress = String.format(&quot;%s@%s&quot;, new Object[] {&quot;${toUserName}&quot;, &quot;${domain}&quot;});

log.info(&quot;Sending message from {} to {}&quot;, fromAddress, toAddress);
try {
  sendOperation.sendMessage(fromAddress, toAddress);
} catch(Exception ex) {
	log.error(&quot;An error has been occured&quot;, ex);
	throw ex;
}
log.info(&quot;Message sent to {} from {} successfully.&quot;, toAddress, fromAddress);

</stringProp>
          <stringProp name="BeanShellSampler.filename"></stringProp>
